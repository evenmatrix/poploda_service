services:
  BranchService:
    config:
      name: branch
      server_host: <%= server_host %>
      server_port: <%= server_port %>
      sub_domain: <%= sub_domain %>
      secret: <%= secret %>
      env: <%= env %>
      OPENTOK_API_KEY: <%= opentok_api_key %> 
      OPENTOK_API_SECRET: <%= opentok_api_secret %>
      db: <%= shared_path %>/config/
      log_path: <%= shared_path %>/log/
